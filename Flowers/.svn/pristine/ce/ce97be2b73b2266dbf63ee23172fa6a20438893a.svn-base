   height:46px;
}
.wjmm_table td span{
    width: 80px;
    display: inline-block;
    text-align: right;
    padding-right: 3px;
}
.wjmm_txt:focus {
    border: 2px solid #b3b3b3;
    outline: none;
}
.register_nav_86{
	width:50px;
	line-height:46px; 
