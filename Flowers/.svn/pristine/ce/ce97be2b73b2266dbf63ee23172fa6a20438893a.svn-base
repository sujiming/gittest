    right: 24px;
}

.wjmm_table {
	width: 400px;
    font-size: 14px;
    margin: 80px auto 0;
}

.y_time {
    text-align: center;
}

.wjmm_table td {
    line-height: 25px;
    padding: 10px 0px;
    position: relative;
}

.wjmm_txt {
    width: 288px;
    border: 1px solid #b3b3b3;
    padding: 14px 5px;
