}
/*找回密码*/

.findok {
    width: 960px;
    text-align: center;
    margin-top: 200px;
    float: left;
}

.zhmm_con {
    width: 960px;
    min-height: 230px;
    line-height: 30px;
    height: 125px;
    text-align: center;
    margin-top: 50px;
    float: left;
}
/*404错误*/

.error {
    width: 960px;
    text-align: center;
    margin: 100px 0;
    float: left;
}

.error_box {
    min-height: 286px;
    height: 286px;
    border: 1px solid #e0e0e0;
    padding: 62px 0px;
    background: #fff;
}

.error_img {
    text-align: center;
}

.error_txt {
    text-align: center;
    line-height: 30px;
    color: #999;
}

.error_txt a {
    text-decoration: underline;
}
/*验证身份***************/

.yz_item,
.yz_jym {
    float: left;
    color: #b3b3b3;
    font-size: 14px;
}

.yz_item {
    display: block;
    margin-right: 5px;
    margin-left: 20px;
    color:#000;
}

.yz_phone {
    color: #414141;
    font-size: 16px;
    line-height: 15px;
}

.yz_jym_txt {
    width: 158px;
    margin-left: 10px;
   height:46px;
    font-size: 14px;
    border:1px solid #b3b3b3;
    padding-left:5px
}

.yz_jym_txt:focus {
    border: 2px solid #b3b3b3;
    outline: none;
}

.yz_jym {
    height: 40px;
    line-height: 45px;
    display: block;
    color:#414141
}

.hq_jym {
    width: 120px;
    height: 46px;
    background: #d9d9d9;
    line-height: 46px;
    display: block;
    float: right;
    text-align: center;
    cursor: pointer;
}
/*3----------4*/

.find_password {
    font-size: 14px;
    color: #000;
    width: 465px;
    margin: 0 auto;
}

.find_password input {
    width: 290px;
    border: solid 1px #b3b3b3;
    display: inline-block;
    outline: none;
    text-indent: 5px;
    padding-top: 14px;
    padding-bottom: 14px;
    font-size: 14px;
}

.find_password input:focus {
    border: 2px solid #b3b3b3;
    outline: none;
}

.find_password_font {
    width: 78px;
    display: inline-block;
    text-align: left;
}

.find_password_hint {
    display: block;
    font-size: 14px;
    margin-left: 88px;
    line-height: 30px;
}

.find_password_button {
    color: #fff;
    width: 290px;
    height: 50px;
    line-height: 50px;
    display: block;
    margin: 50px auto;
    text-align: center;
    background: #414141;
    letter-spacing: 15px;
    text-decoration: none;
}

.find_password_button:hover {
    background: #676767;
}

.find_win {
    width: 295px;
    height: 60px;
    line-height: 30px;
    padding-left: 60px;
    margin: 0 auto;
    background: url(http://www.roseonly.com.cn/upload/syspic_new/14683094247893418.png) no-repeat scroll 0 center;
    color: #000;
    font-size: 14px;
}

.find_win_f1 {
    font-size: 16px;
    font-weight: bold;
}

.find_win_f2 {
    color: #414141;
}

.find_win_f2 a {
    color: #000;
    text-decoration: none;
    font-weight: bold;
}
.press {
    width: 450px;
    height: 10px;
    background: #c9c9c9;
    margin: 10px auto;
    position: relative;
    margin-bottom: 50px
}

.press .press_all {
    width: 100%;
    height: 10px;
}

.press .press_all .line{
    height: 10px;
    float:left;
}

.press_now {
    background: #414141;
}

.press .node{
    top: -12px;
    width: 35px;
    height: 35px;
    line-height: 35px;
    font-size: 18px;
    color: #FFF;
    background: #414141;
    position: absolute;
    border-radius: 35px;
    text-align: center;
}
/*ipad竖屏*/
@media all and (orientation : portrait){
    .series,.ul{  width:768px  }
    .head,.container,.menus2,.footer_box{width:768px}
    .logo-container,.footer,.nav-container{min-width: 768px}
    #nav li{margin:0 0px;width: 104px}
    #nav .topmenu_1{font-size: 12px;}
    .li_img img{width: 192px;height:192px;  }
    .li_img{width: 192px;height: 180px;margin: 24px auto;}
    .menus2 .number1{width:80px}
    .menus_pic{width:480px;height:198px;right:220px}
    .menus{width:990px}
    .menus_pic img{width:100%;height:100%}
    .new_con1 div img{width:100%}
    .previewShowWindow{width:300px;height:300px;border:none}
    .previewShowWindow img{width:300px;height:300px}
    .footer_box ul {padding: 0 30px}
    .footer_box ul p{font-size: 14px;}
    .footer_box ul li a{font-size: 12px;}
    .new_r a img{width: 100px;height: 100px}
    .rose_select a{margin:0 10px}
    .rose_select_input{width:160px}
    .menus2 span{font-size: 12px;}
     #nav{width:768px}
}