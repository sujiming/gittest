    text-align: center;
}

.wjmm_con {
    width: 300px;
    margin: 0 auto;
    line-height: 22px;
    padding: 7px 0;
}

.xyb_btn {
    background: #2a2a2a;
    width: 288px;
    height: 46px;
    text-align: center;
    line-height: 46px;
    color: #fff;
    font-weight: bold;
    border: none;
    font-size: 16px;
