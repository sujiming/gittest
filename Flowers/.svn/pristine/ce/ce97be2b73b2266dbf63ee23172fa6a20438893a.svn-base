@charset "utf-8";
/*忘记密码*/

input:focus {
    border: 2px solid #b3b3b3;
}
select {
	background: #fff;
  border: solid 1px #b3b3b3;
  appearance:none;
  -moz-appearance:none;
  -webkit-appearance:none;
  border-radius: 0;
-webkit-border-radius: 0;
-moz-border-radius: 0;
-khtml-border-radius: 0;
  background: url("http://www.roseonly.com.cn/upload/syspic_new/14836910765083806.png") no-repeat scroll 265px center transparent;
}
select::-ms-expand { display: none; }
option{
		 		border: solid 1px #b3b3b3;
			  appearance:none;
			  -moz-appearance:none;
			  -webkit-appearance:none;
			  border-radius: 0;
				-webkit-border-radius: 0;
				-moz-border-radius: 0;
				-khtml-border-radius: 0;
				background:#fff;
		}     
.find_line {
    border-top: 1px solid #dadada;
    width: 100%;
    height: 1px;
    margin-top: 130px;
}

.pess_p {
    position: absolute;
    font-size: 14px;
    top: 20px;
}

.press .p_1 {
    left: -30px;
}

