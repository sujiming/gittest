$.prototype.addOption = function(cache, name, val) {  
    var tag = this[0],  
    add = function(result) {  
        $.each(result, function() {  
            tag.options.add(new Option(this[name], this[val]))  
        });  
    };  
    tag.options.length = 0;  
    tag.options.add(new Option('--请选择--', ''));  
    add(cache);  
    return this  
};   