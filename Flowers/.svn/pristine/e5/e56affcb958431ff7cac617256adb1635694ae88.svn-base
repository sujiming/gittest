package com.ghj.model;
/*
 * 信息类
 */
public class MessageModel {
	private String msg;
	private String code;
	public MessageModel() {
		
	}
	public MessageModel(String code, String msg) {
		super();
		this.msg = msg;
		this.code = code;
	}
	public String getMsg() {
		return msg;
	}
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	
}
