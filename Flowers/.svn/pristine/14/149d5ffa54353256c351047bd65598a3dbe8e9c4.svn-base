.mod_address ul{width:100%;;margin:10px 0 0 0;}
.mod_address li{border:1px #EAEAEA solid;float:left;display:block;width:100%;height:150px;margin:10px 0 0 0;position:relative;z-index:1;left:0;top:0;}
.mod_address li:hover,.mod_address li.hover{border:1px #EAEAEA solid;-moz-box-shadow:0 0 5px #EAEAEA;-webkit-box-shadow:0 0 5px #EAEAEA;box-shadow:0 0 5px #EAEAEA;}
.mod_address li span{display:block;float:left;width:100%;}
.mod_address li .icon_com{position:relative;z-index:2;left:0;top:0;width:40px;height:150px;}
.mod_address li .icon_com i{display:block;width:40px;height:150px;background-color:#83848e;filter:alpha(opacity=40);opacity:.4;}
.mod_address li .icon_com em{font-style:normal;color:#fff;font-size:14px;font-family:'Microsoft YaHei';position:absolute;left:12px;top:45px;z-index:3;display:block;width:15px;height:60px;line-height:1em;}
.mod_address li .icon_key{position:relative;z-index:2;left:0;top:0;width:40px;height:150px;margin-left:-1px;}
.mod_address li .icon_key i{display:block;width:40px;height:150px;background-color:#ff875a;filter:alpha(opacity=20);opacity:.2;}
.mod_address li .icon_key em{font-style:normal;color:#ff875a;font-size:14px;font-family:'Microsoft YaHei';position:absolute;left:12px;top:30px;z-index:3;display:block;width:15px;height:90px;line-height:1em;}
.mod_address li .nickname{width:72%;font-weight:bold;color:#333;font-family:'Microsoft YaHei';font-size:16px;margin:15px 0 0 30px;}
